{
	"name": "klaversjassen",
	"version": "0.0.1",
	"author": "forge-42",
	"private": true,
	"sideEffects": false,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"execute": "tsx",
		"build": "rm -Rf build ; pnpm run codegen ; react-router build",
		"dev": "react-router dev",
		"start": "NODE_ENV=production node ./build/server/index.js",
		"test": "vitest run --browser.headless",
		"test:ui": "vitest",
		"test:cov": "vitest run --coverage --browser.headless",
		"typecheck": "tsc",
		"validate": "pnpm run check && pnpm run typecheck && pnpm run test && pnpm run check:unused",
		"check": "biome check .",
		"check:fix": "biome check --fix .",
		"check:unused": "knip --max-issues 0",
		"check:unused:fix": "knip --fix",
		"typegen": "react-router typegen ; wrangler types --env-interface CloudflareBindings",
		"deploy": "rm -Rf .wrangler ; npm run build && wrangler deploy",
		"codegen": "rm -Rf .generated-sources/openapi ; openapi-generator-cli generate -i https://api.klaversjassen.nl/schema/api.yaml -g typescript-fetch -o .generated-sources/openapi --additional-properties=supportsES6=true,typescriptThreePlus=true,useSingleRequestParameter=false",
		"codegen-help": "openapi-generator-cli config-help -g typescript-fetch"
	},
	"dependencies": {
		"@emotion/react": "11.14.0",
		"@emotion/styled": "11.14.0",
		"@epic-web/client-hints": "1.3.5",
		"@fontsource/roboto": "5.2.5",
		"@forge42/seo-tools": "1.4.0",
		"@mui/icons-material": "7.1.0",
		"@mui/material": "7.1.0",
		"@mui/x-date-pickers": "8.3.0",
		"@tanstack/react-query": "5.76.0",
		"clsx": "2.1.1",
		"dayjs": "1.11.13",
		"hono": "4.7.9",
		"i18next": "25.1.2",
		"i18next-browser-languagedetector": "8.1.0",
		"i18next-http-backend": "3.0.2",
		"isbot": "5.1.28",
		"notistack": "3.0.2",
		"phaser": "3.88.2",
		"pretty-cache-header": "1.0.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-i18next": "15.5.1",
		"react-oidc-context": "3.3.0",
		"react-router": "7.6.0",
		"react-router-hono-server": "2.13.0",
		"react-use-websocket": "4.13.0",
		"remix-hono": "0.0.18",
		"remix-i18next": "7.2.1",
		"tailwind-merge": "3.3.0",
		"zod": "3.24.4"
	},
	"devDependencies": {
		"@babel/preset-typescript": "7.27.1",
		"@biomejs/biome": "1.9.4",
		"@cloudflare/workers-types": "4.20250513.0",
		"@openapitools/openapi-generator-cli": "2.20.0",
		"@react-router/dev": "7.6.0",
		"@tailwindcss/vite": "4.1.6",
		"@testing-library/react": "16.3.0",
		"@types/node": "22.15.17",
		"@types/react": "19.1.4",
		"@types/react-dom": "19.1.4",
		"@vitest/browser": "3.1.2",
		"@vitest/coverage-v8": "3.1.2",
		"@vitest/ui": "3.1.2",
		"babel-plugin-react-compiler": "19.1.0-rc.1",
		"happy-dom": "17.4.7",
		"knip": "5.55.1",
		"lefthook": "1.11.12",
		"playwright": "1.52.0",
		"react-router-devtools": "5.0.5",
		"tailwindcss": "4.1.6",
		"tsx": "4.19.4",
		"typescript": "5.8.3",
		"vite": "6.3.5",
		"vite-plugin-babel": "1.3.1",
		"vite-plugin-icons-spritesheet": "3.0.1",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.1.2",
		"vitest-browser-react": "0.1.1",
		"workbox-core": "7.3.0",
		"workbox-precaching": "7.3.0",
		"workbox-routing": "7.3.0",
		"wrangler": "4.14.4"
	},
	"packageManager": "pnpm@10.10.0",
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.34.3"
	},
	"engines": {
		"node": ">=22.12.0",
		"pnpm": ">=10.2.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "lefthook", "msw"]
	}
}
