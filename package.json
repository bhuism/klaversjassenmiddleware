{
	"name": "klaversjassen",
	"version": "0.0.1",
	"author": "bhuism",
	"private": true,
	"sideEffects": false,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"execute": "tsx",
		"build": "rm -Rf build dist ; pnpm run codegen ; GITHUB_SHA=$(git rev-parse HEAD) GITHUB_DATE=$(git show --no-patch --format=%cI) react-router build",
		"dev": "GITHUB_SHA=$(git rev-parse HEAD) GITHUB_DATE=$(git show --no-patch --format=%cI) react-router dev",
		"start": "react-router-serve ./build/server/index.js",
		"test": "vitest run --browser.headless",
		"test:ui": "vitest",
		"test:cov": "vitest run --coverage",
		"typecheck": "pnpm run typegen && tsc",
		"validate": "pnpm run codegen && pnpm run check && pnpm run typecheck && pnpm run test && pnpm run check:unused",
		"check": "biome check .",
		"check:fix": "biome check --fix .",
		"check:unused": "knip --max-issues 10",
		"check:unused:fix": "knip --fix",
		"typegen": "react-router typegen",
		"deploy": "rm -Rf .wrangler ; GITHUB_SHA=$(git rev-parse HEAD) GITHUB_DATE=$(git show --no-patch --format=%cI) npm run build && wrangler pages deploy build/client --project-name=klaversjassen --commit-dirty=true",
		"codegen": "rm -Rf .generated-sources/openapi ; openapi-generator-cli generate -i https://api.klaversjassen.nl/schema/api.yaml -g typescript-fetch -o .generated-sources/openapi --additional-properties=supportsES6=true,typescriptThreePlus=true,useSingleRequestParameter=false",
		"codegen-help": "openapi-generator-cli config-help -g typescript-fetch"
	},
	"dependencies": {
		"@emotion/react": "11.14.0",
		"@emotion/styled": "11.14.0",
		"@fontsource/roboto": "5.2.6",
		"@mui/icons-material": "7.1.1",
		"@mui/material": "7.1.1",
		"@mui/x-data-grid": "8.5.1",
		"@mui/x-date-pickers": "8.5.1",
		"@react-router/node": "7.6.2",
		"@react-router/serve": "7.6.2",
		"@tanstack/react-query": "5.80.6",
		"@toolpad/core": "0.15.0",
		"clsx": "2.1.1",
		"dayjs": "1.11.13",
		"isbot": "^5",
		"notistack": "3.0.2",
		"oidc-client-ts": "3.2.1",
		"phaser": "3.90.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-oidc-context": "3.3.0",
		"react-router": "7.6.2",
		"react-use-websocket": "4.13.0",
		"tailwind-merge": "3.3.0",
		"workbox-window": "7.3.0"
	},
	"devDependencies": {
		"@babel/preset-typescript": "7.27.1",
		"@biomejs/biome": "1.9.4",
		"@openapitools/openapi-generator-cli": "2.20.2",
		"@react-router/dev": "7.6.2",
		"@tailwindcss/vite": "4.1.8",
		"@testing-library/react": "16.3.0",
		"@types/node": "22.15.31",
		"@types/react": "19.1.7",
		"@types/react-dom": "19.1.6",
		"@vite-pwa/assets-generator": "1.0.0",
		"@vitest/browser": "3.2.3",
		"@vitest/coverage-v8": "3.2.3",
		"@vitest/ui": "3.2.3",
		"babel-plugin-react-compiler": "19.1.0-rc.2",
		"happy-dom": "17.6.3",
		"knip": "5.60.2",
		"lefthook": "1.11.13",
		"playwright": "1.52.0",
		"react-router-devtools": "5.0.6",
		"tailwindcss": "4.1.8",
		"tsx": "4.19.4",
		"typescript": "5.8.3",
		"vite": "6.3.5",
		"vite-plugin-pwa": "1.0.0",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.3",
		"vitest-browser-react": "0.2.0",
		"wrangler": "4.19.1"
	},
	"packageManager": "pnpm@10.12.1",
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.34.3"
	},
	"engines": {
		"node": ">=22.12.0",
		"pnpm": ">=10.2.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "lefthook", "msw"]
	}
}
